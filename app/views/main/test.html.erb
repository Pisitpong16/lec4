<h1>Main#test</h1>
<%= form_with url: "/main/test", method:'post',data:{turbo:false} do |form| %>
    <%= form.label :query, "SUBJECT SCORE" %><br>
    
    <%= form.text_field :subject1 %>
    <%= form.text_field :score1 %><br>
    <%= form.text_field :subject2 %>
    <%= form.text_field :score2 %><br>
    <%= form.text_field :subject3 %>
    <%= form.text_field :score3 %>
    <%= form.submit "Submit" %>

  <% end %>
